{
  "expo": {
    "name": "Flight Booking",
    "slug": "flight-booking-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconungdung.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/anhkhoichay.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "11da1eab-2ab1-4c52-8e6e-c83f54dbd913"
      }
    }
  }
}
